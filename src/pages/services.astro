---
import { getCollection } from "astro:content";
import { Image } from 'astro:assets';
import { Icon } from "astro-icon";
import Layout from "@layouts/Layout.astro";
import Service from "@components/services.astro";
import Container from "@components/container.astro";
import Sectionhead from "@components/sectionhead.astro";
import '../styles/custom.css';
import type { AstroCookies } from "astro";


---

<Layout title="Services">
  <Container>

    <div class=" mx-4 mt-16 mb-2 py-16 rounded-3xl md:rounded-full border-4 bg-orange-50 border-slate-800 max-w-screen-xl">
      <Sectionhead>
        <Fragment slot="title">services.</Fragment>
        <Fragment slot="desc">If you don't see a specific service you are looking for 
          please don't hesitate to reach out and ask. We most likely handle it. </Fragment>
      </Sectionhead>
    </div>


    <Service />

  </Container>
</Layout>
