---
// @ts-ignore
import { Image } from 'astro:assets';
import { Icon } from "astro-icon";
import Container from "@components/container.astro";
import LottieAnimation from "astro-integration-lottie/Lottie.astro";
import Link from "@components/ui/link.astro";
import splash from "@assets/cheerStatic/boomSplash.png";
import hand2 from "@assets/cheerStatic/handPoint2.png";
import spark from "@assets/cheerStatic/spark1.png";



  function getClassByDescription(number) {
    if (number === "01") {
      return "bg-[--primary-color]";
    } else if (number === "02") {
      return "bg-[--secondary-color]";
    }
    return "bg-[--contrast-color]";
  }


