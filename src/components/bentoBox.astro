<script is:inline>
  document.addEventListener("DOMContentLoaded", function () {

  const targetSection = document.getElementById("bentoSection");
  const spanHeadBento = document.getElementById("bentoSpan");
  const leadHeadBento = document.getElementById("bentoLead");
  const textTarget = document.getElementById("textScroll");
  const textTargetContain = document.getElementById("textScrollContainer");

  
  document.addEventListener("scroll", function () {
    const parentScrollRect = textTargetContain.getBoundingClientRect();
    let adjust = 0;

   if (parentScrollRect.top < window.innerHeight-940 && parentScrollRect.bottom-200 >= 0) {
    let finalTop = (Math.round(parentScrollRect.top)*-1);
    if (finalTop > 0) adjust = 40; else adjust = 0;
    textTarget.style.top = finalTop + adjust + "px"; 

   }

    const targetPos1 = targetSection.getBoundingClientRect().top;
    if (targetPos1 <= -705) {
      targetSection.style.backgroundColor = "#dfd9cc"; 
      spanHeadBento.style.color = "#8d7dff";
      leadHeadBento.style.color = "#22212d";
    } else { // before scrolling down
      targetSection.style.backgroundColor = "#121309";
      spanHeadBento.style.color = "#ffe757";
      leadHeadBento.style.color = "#fcf9e4";
    }
  });
});
</script>