---
// @ts-ignore
import { Image } from 'astro:assets';
import { Icon } from "astro-icon";
import downLight from "@assets/cheerStatic/downLight.png";
import bolt from "@assets/cheerStatic/bolt.png";
import wizard from "@assets/cheerVector/cheerWizard.svg";
import stars from "@assets/cheerStatic/starsBg2.png";
import Container from "@components/container.astro";
import LottieAnimation from "astro-integration-lottie/Lottie.astro";


  function getClassByDescription(number) {
    if (number === "01") {
      return "bg-zinc-800";
    } else if (number === "02") {
      return "bg-[--secondary-color]";
    }
    return "bg-[--contrast-color]";
  }


---

<div class="pt-18 pb-64 relative overflow-hidden mt-[-2px] md:mt-0" id="bentoSection">

  <div class="w-28 h-64 md:h-96 md:w-72 absolute top-[-10px] left-6 z-20 rotate-[341deg] opacity-20">
    <Image
      src={downLight}
      alt="down arrow light"
      class="w-full h-full object-contain"
      loading="lazy"
      format="avif"
    />
  </div>

  <div class="w-28 h-64 md:h-96 md:w-72 absolute top-64 md:right-10 right-3 z-20 rotate-12 opacity-20">
    <Image
      src={bolt}
      alt="bolt"
      class="w-full h-full object-contain"
      loading="lazy"
      format="avif"
    />
  </div>

  <div id="textScrollContainer" class="min-h-fit h-[40rem] w-full relative">
    <h2 id="textScroll" class="absolute max-h-96 mt-20 text-5xl md:text-7xl text-font-white 
    -rotate-12 top-10 left-20 md:left-[42%]">
